programButton.addEventListener('click', function() {
if (programPopup.style.display === 'block') {
programPopup.style.display = 'none';
} else {
programPopup.style.display = 'block';
}
});


// Close popup when clicking outside
document.addEventListener('click', function(event) {
    if (!programButton.contains(event.target) && !programPopup.contains(event.target)) {
        programPopup.style.display = 'none';
    }
});
closeBtn1.addEventListener('click', () => {
    programPopup.style.display = 'none';
});
