document.getElementById('applicationForm1').addEventListener('submit', function(event) {
            event.preventDefault(); // Prevent default form submission

            const fullName = document.getElementById('fullName').value;
            const mobile = document.getElementById('mobile').value;
            const program = document.getElementById('program').value;
            const state = document.getElementById('state').value;
            const email = document.getElementById('email').value;
            const form = document.forms['applicationForm1'];
            


            // Basic validation (you can add more robust validation)
            if (!fullName || !mobile || !program || !email || !state ) {
                alert('Please fill in all fields.');
                return;
            }

            // You can add code here to send the data to a server or perform other actions
            

            form.addEventListener('submit', e => {
