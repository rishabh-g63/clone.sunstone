           fetch(scriptURL, { method: 'POST', body: new FormData(form)}) // Uses data from form1
          .then(response => alert("Thank you! Form is submitted"  ))
         .then(() => {window.location.reload(); })
         .catch(error => console.error('Error!', error.message));

         console.log('Form submitted:', { fullName, mobile, program , email, state });


});

            form.addEventListener('click', () => {
            applicationForm1.style.closeBtn = 'close';
